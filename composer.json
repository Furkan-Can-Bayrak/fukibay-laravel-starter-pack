{
  "name": "fukibay/laravel-starter-pack",
  "description": "Starter pack: minimal scaffolding helpers for new Laravel projects.",
  "type": "laravel-package",
  "license": "MIT",
  "keywords": ["laravel", "repository", "service", "pattern", "starter", "scaffolding"],
  "authors": [
    {
      "name": "Furkan Can Bayrak",
      "email": "furkancanb957@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0 || ^11.0",
    "illuminate/database": "^10.0 || ^11.0",
    "illuminate/pagination": "^10.0 || ^11.0",
    "illuminate/console": "^10.0 || ^11.0",
    "illuminate/filesystem": "^10.0 || ^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "orchestra/testbench": "^8.22|^9.0",
    "nunomaduro/larastan": "^2.9",
    "laravel/pint": "^1.16"
  },
  "autoload": {
    "psr-4": {
      "Fukibay\\StarterPack\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fukibay\\StarterPack\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fukibay\\StarterPack\\StarterPackServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit",
    "analyse": "vendor/bin/phpstan analyse",
    "format": "vendor/bin/pint"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}